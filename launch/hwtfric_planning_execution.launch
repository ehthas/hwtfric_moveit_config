<launch>

  <rosparam command="load" file="$(find hwtfric_moveit_config)/config/joint_names.yaml"/>

  <include file="$(find hwtfric_moveit_config)/launch/planning_context.launch">
   <arg name="load_robot_description" value="true"/>
  </include>

  <node name="joint_state_publisher" pkg="joint_state_publisher" type="joint_state_publisher">
    <param name="/use_gui" value="false"/>
    <rosparam param="/source_list">[/mhuman/joint_states]</rosparam>
  </node> 

  <node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher" respawn="false" output="screen">
    <remap from="/joint_states" to="/mhuman/joint_states" /> 
  </node>

  <include file="$(find hwtfric_moveit_config)/launch/move_group.launch">
   <arg name="publish_monitored_planning_scene" value="true"/>
  </include>

  <include file="$(find hwtfric_moveit_config)/launch/moveit_rviz.launch">
   <arg name="config" value="true"/>
  </include>

</launch>
